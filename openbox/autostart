#!/usr/bin/env bash

## Kill if already running
killall -9 xfsettingsd polybar dunst xfdesktop xfce4-panel ksuperkey xfce4-power-manager

## Restore Wallpaper
##nitrogen --restore

## xfce4-settings daemon
xfsettingsd &

## Enable power management
xfce4-power-manager &

## xfdesktop ##
xfdesktop &

## Start numlockx
exec numlockx &

## diodon
exec diodon &

## Compositor
exec xcompmgr &

## Polybar
exec polybar &

## Notification Daemon
exec dunst &

## Launch panel
exec xfce4-panel &

## Thunar Daemon
exec thunar --daemon &

## Polkit-Gnome ##
exec /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1

"$HOME"/.config/openbox/autostart
